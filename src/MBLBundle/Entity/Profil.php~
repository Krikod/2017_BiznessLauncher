<?php

namespace MBLBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
/**
 * Profil
 */
class Profil extends BaseUser
{
//    /**
//     * @var
//     */
//    protected $id;

//    /**
//     * @var
//     */
//    protected $bioUrl;

    public function setEmail($email)
    {
        $email =is_null($email) ? '' : $email;
        parent::setEmail($email);
        $this->setUsername($email);
        return $this;
    }

    // Generated code



}
